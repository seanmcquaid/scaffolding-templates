{
  "name": "expo-react-native",
  "version": "0.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "EXPO_ROUTER_APP_ROOT=src/app expo run:android",
    "build": "EXPO_ROUTER_APP_ROOT=src/app expo export --output-dir ./build --platform all",
    "bundlesize": "bundlesize",
    "dev": "EXPO_ROUTER_APP_ROOT=src/app expo start",
    "ios": "EXPO_ROUTER_APP_ROOT=src/app expo run:ios",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint . --fix",
    "prepare": "is-ci || husky",
    "start": "EXPO_ROUTER_APP_ROOT=src/app expo start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "web": "EXPO_ROUTER_APP_ROOT=src/app expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.6",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-router": "~6.0.3",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "i18next": "^25.4.2",
    "ky": "^1.9.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.7.3",
    "react-native": "0.76.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-web": "~0.21.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.34.0",
    "@expo/metro": "^0.1.1",
    "@expo/metro-runtime": "~6.1.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/react": "~18.3.0",
    "@types/react-dom": "~18.3.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "bundlesize2": "^0.0.35",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=22.12.0"
  }
}